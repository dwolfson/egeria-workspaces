          "Logging": {
                "console_filter_levels": [
                  "ERROR",  "WARNING", "INFO"
                ],
                "console_logging_enabled": [
                ],
                "console_logging_level": "INFO",
                "enable_logging": true,
                "file_logging_level": "INFO",
                "log_directory": "/home/jovyan/work/logs",
                "logging_console_format": "<green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green> | <level>{level: <8}</level> | <cyan>{name}</cyan>:<cyan>{line}</cyan> - <level>{message}</level>",
                "logging_file_format": "{time:YYYY-MM-DD HH:mm:ss} | {level} | {module}-{function}:{line}-{message}-{extra}"
              },
        "Debug": {
                "timeout_seconds": 30,
                "debug_mode": false,
                "enable_logger_catch": false,
                "feature_x_enabled": false
        },
        "User Profile": {
